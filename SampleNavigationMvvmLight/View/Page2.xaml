<Page x:Class="Chargily.View.Page2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chargily"
        mc:Ignorable="d"
        Title="Page2" Height="300" Width="300" DataContext="{Binding Path=Page2ViewModel, Source={StaticResource Locator}}">
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding Page2Text}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25"/>
            <Button Command="{Binding HomeCommand}" Content="Home"/>
        </StackPanel>
    </Grid>
</Page>
