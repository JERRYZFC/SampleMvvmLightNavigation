<Page x:Class="Chargily.View.Home"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Chargily.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Home" DataContext="{Binding  HomeViewModel,Source={StaticResource Locator}}">

    <StackPanel>
        <TextBlock Text="{Binding MainPageText}" FontSize="25"/>
        <Button Command="{Binding Page1Command}" Content="Page1"/>
        <Button Command="{Binding Page2Command}" Content="Page2"/>
    </StackPanel>
</Page>
