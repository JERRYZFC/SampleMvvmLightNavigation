<Page x:Class="Chargily.View.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Chargily.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page1" Height="300" Width="300" DataContext="{Binding Path=Page1ViewModel, Source={StaticResource Locator}}">
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding Page1Text}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25"/>
            <Button Command="{Binding HomeCommand}" Content="Home"/>
        </StackPanel>
    </Grid>
</Page>
